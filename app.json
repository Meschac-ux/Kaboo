{
  "expo": {
    "name": "kaabo",
    "slug": "kaabo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kaabo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/kaboo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffbb04"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffbb04"
      },
      "package": "com.hope2023.kaabo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "506e8da0-ff11-45f3-8e2b-bde7ae97bad3"
      }
    }
  }
}
